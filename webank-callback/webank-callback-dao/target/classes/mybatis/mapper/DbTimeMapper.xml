<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.momo.dao.DbTimeMapper">
	<select id="querySystemTime" resultType="String">
		select
		date_format(now(),'%Y%m%d%H%i%S') as sysdt
	</select>
	
	<select id="querySystemDate" resultType="String">
		select
		date_format(now(),'%Y%m%d') as sysdt
	</select>
</mapper>